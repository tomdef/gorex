{
	"folder": ".\\example",
	"filter": "F1.txt",
	"scopes": [
		{
			"name": "example-find-any-command-in-scope",
			"startQuery": "^\\W*BEGIN\\W*$",
			"finishQuery": "^\\W*(END)\\W*$",
			"startQueryCloseScope": true,
			"searchQuery": [
				"^\\s*COMMAND\\=.*$"
			],
			"searchQueryMode": 1
		}
	]
}