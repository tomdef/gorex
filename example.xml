<ScanConfig folder=".\example" filder="*.txt">
	<scopes name="example-find-any-command-in-scope">
		<startQuery>^\W*BEGIN$</startQuery>
		<finishQuery>^\W*END$</finishQuery>
		<searchQuery>^\s*COMMAND\=.*$</searchQuery>
		<searchQueryMode>1</searchQueryMode>
	</scopes>
	<scopes name="example-find-two-commands-in-scope">
		<startQuery>^\W*BEGIN$</startQuery>
		<finishQuery>^\W*END$</finishQuery>
		<searchQuery>^\s*COMMAND\=AAA$</searchQuery>
		<searchQuery>^\s*COMMAND\=BBB$</searchQuery>
		<searchQueryMode>0</searchQueryMode>
	</scopes>
</ScanConfig>